{
    "name": "@types/thalassa",
    "version": "0.1.0",
    "private": true,
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "scripts": {
        "build": "tsc",
        "type-check": "tsc --noEmit",
        "generate:api": "cross-env NODE_OPTIONS=--no-deprecation openapi-generator-cli generate -i ../../openapi.yaml -g typescript-fetch -o ./src/api --additional-properties=supportsES6=true,modelPropertyNaming=original,typescriptThreePlus=true,enumPropertyNaming=original"
    },
    "dependencies": {
        "typescript": "^5.9.2"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.23.1",
        "cross-env": "^10.0.0",
        "glob": "^10.3.10",
        "prettier": "^3.6.2"
    }
}
